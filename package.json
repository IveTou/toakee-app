  "name": "toakee-web-app",
  "version": "1.0.0",
  "description": "Node Express React Simplest Application",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "NODE_ENV=production babel-node ./src/server/index.js",
    "script": "babel-node scripts/index",
    "webpack:watch": "webpack -w",
    "express-server": "SSR=true babel-node ./src/server/index.js",
    "prebuild": "npm run lint",
    "build": "npm run build:app",
    "build:app": "NODE_ENV=production webpack -p",
    "build:server": "NODE_ENV=production babel src/server/ -d dist/",
    "dev": "concurrently --kill-others  \"npm run webpack:watch\" \"npm run express-server\"",
    "lint": "eslint --ext .js,.jsx src/**"
  },
  "keywords": [
    "express",
    "react",
    "node"
  ],
